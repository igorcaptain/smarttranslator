<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>SmartTranslator</TargetDatabaseName>
    <DeployScriptFileName>SmartTranslator.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=(local)\SQLEXPRESS;Persist Security Info=True;User ID=ST_Admin;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False</TargetConnectionString>
